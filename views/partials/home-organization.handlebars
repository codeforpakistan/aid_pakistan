                <div class="tab-pane fade in active" id="tab1">
                    <!-- Start Recent Projects Carousel -->
                    <ul id="portfolio-list" data-animated="fadeIn">
                        {{#each all}}
                        <li>
                        <img src={{this.pictures.gallery.[0]}}?dim=250x200  alt="Image not found" onError="this.onerror=null;this.src='http://dummyimage.com/250x200/00bb57/fff.jpg&text=image+not+available';" />
                        <div class="portfolio-item-content">
                            <span class="header">About {{this.name}}</span>
                            <p class="body">{{this.description}} </p>
                        </div>

                        <h4 style="margin-top:10px; ">{{this.name}}</h4>
                        <a href="#organization/{{this.id}}"><i class="more">+</i></a>

                        </li>
                        {{/each}}

                    </ul>
                    <!-- End Recent Projects Carousel -->
